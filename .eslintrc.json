{
    "env": {
        "browser": true,
        "es2021": true,
        "es6": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "function-paren-newline": ["error", "consistent"],
        "import/extensions": 0,
        "no-param-reassign":"off" 
       
}
}